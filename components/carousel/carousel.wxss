.swiperClass {
  margin: 0;
}

.slide-navigator {
  width: 100%;
  height: 75%;
  position: relative;
}

.slide-navigator.one-img {
  height: 100%;
}

.active {
  transform: none;
  transition: all 0.2s ease-in 0s;
}

.quiet {
  transform: scale(0.90);
  transition: all 0.2s ease-in 0s;
}

.slide-image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
